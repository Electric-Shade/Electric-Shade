#include<stdio.h>
main(){
	int a[5],loc=0,i=0,key;
	printf("Enter values\n");
	for(i=0;i<5;i++)
	{
		scanf("%d",&a[i]);
	}
	
	printf("enter the number to search");
	scanf("%d",&key);
	
	for(i=0;i<5;i++)
	{
		if(a[i]==key){
			loc=i+1;
			break;
		}
	}
	if(loc!=0){
	printf("%d",loc);}
	else
	printf("value non-existent");
}
